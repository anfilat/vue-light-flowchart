{
  "name": "vue-simple-flowchart",
  "version": "0.1.8",
  "private": false,
  "main": "./dist/vue-flowchart.common.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest demo",
    "lint": "vue-cli-service lint",
    "build:bundle": "vue-cli-service build --target lib --name vue-flowchart ./src/index.js",
    "deploy": "gh-pages -d demo"
  },
  "license": "MIT",
  "dependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "gh-pages": "^2.1.1",
    "node-sass": "^4.9.3",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jeffreyrn/vue-simple-flowchart"
  }
}
